{
    "name": "arokettu/uuid-doctrine",
    "type": "library",
    "license": "MIT",
    "description": "Doctrine support for arokettu/uuid",
    "keywords": ["uuid", "ulid", "doctrine"],
    "homepage": "https://sandfox.dev/php/uuid/doctrine.html",
    "authors": [{
        "name": "Anton Smirnov",
        "email": "sandfox@sandfox.me",
        "role": "developer",
        "homepage": "https://sandfox.me/"
    }],
    "support": {
        "issues": "https://gitlab.com/sandfox/uuid-doctrine/-/issues",
        "source": "https://gitlab.com/sandfox/uuid-doctrine",
        "docs": "https://arokettu-uuid.readthedocs.io/en/latest/doctrine.html",
        "chat": "https://gitter.im/arokettu/community"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Arokettu\\Uuid\\Doctrine\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Arokettu\\Uuid\\Doctrine\\Tests\\": "tests"
        }
    },
    "require": {
        "php": "^8.2",
        "arokettu/is-resource": "^1.0",
        "arokettu/uuid": "^1.0",
        "doctrine/dbal": "^3.4"
    },
    "require-dev": {
        "arokettu/clock": "^2.2",
        "doctrine/orm": "^2.4",
        "phpunit/phpunit": "^10.2",
        "sandfox.dev/code-standard": "^1.2023.06.29",
        "squizlabs/php_codesniffer": "*",
        "vimeo/psalm": "^5.13"
    },
    "conflict": {
        "doctrine/orm": "< 2.4"
    }
}

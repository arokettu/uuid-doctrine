{
    "name": "arokettu/uuid-doctrine",
    "type": "library",
    "license": "MIT",
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Arokettu\\Uuid\\Doctrine\\": "src"
        }
    },
    "require": {
        "php": "^8.2",
        "arokettu/uuid": "^1.0",
        "doctrine/dbal": "^3.4"
    },
    "require-dev": {
        "doctrine/orm": "^2.4",
        "sandfox.dev/code-standard": "^1.2023.06.29",
        "squizlabs/php_codesniffer": "*"
    },
    "conflict": {
        "doctrine/orm": "< 2.4"
    }
}
